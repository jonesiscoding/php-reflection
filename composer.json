{
    "name": "deviscoding/php-reflection",
    "description": "PHP Library of Reflection-style object classes, with additional functionality for PHP 7.0",
    "type": "library",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "AMJones",
            "email": "am@jonesiscoding.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DevCoding\\Reflection\\": "src/",
            "DevCoding\\Reflection\\Bags\\": "src/Bags",
            "DevCoding\\Reflection\\Comments\\": "src/Comments",
            "DevCoding\\Reflection\\Exception\\": "src/Exceptions",
            "DevCoding\\Reflection\\Tags\\": "src/Tags",
            "DevCoding\\Reflection\\Vars\\": "src/Vars"
        }
    },
    "version": "1.70",
    "config": {
        "platform": {
            "php": "7.0.33"
        }
    },
    "require": {
        "php": ">=7.0",
        "psr/container": "^1.0 || ^2.0",
        "deviscoding/php-polyfill": "^1.85"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0 || ^2.0",
        "symfony/validator": "^3.4 || ^4.4 || ^5.4 || ^6.0 || ^7.0 || ^8.0",
        "symfony/console": "^3.4 || ^4.4 || ^5.4 || ^6.0 || ^7.0 || ^8.0",
        "symfony/property-access": "^3.4 || ^4.4 || ^5.4 || ^6.0 || ^7.0 || ^8.0",
        "doctrine/orm": "^2.5 || ^3.0 || ^3.6"
    }
}
